{
  "name": "cachified",
  "version": "0.0.1-beta.1",
  "description": "neat wrapper for various caches",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/cachified.d.ts",
  "scripts": {
    "prepare": "npm t && rm -rf dist && npm run build",
    "build": "npm run build:declarations && npm run build:esm && npm run build:cjs",
    "build:declarations": "tsc && rm dist/cachified.spec.d.ts",
    "build:esm": "esbuild src/cachified.ts --outfile=dist/index.js --format=esm --target=es2020 --sourcemap --minify",
    "build:cjs": "esbuild src/cachified.ts --outfile=dist/index.cjs --format=cjs --target=es2020 --sourcemap --minify",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Xiphe/cachified.git"
  },
  "keywords": [
    "cache",
    "wrapper",
    "ttl",
    "stale while revalidate",
    "typescript",
    "lru-cache",
    "redis",
    "typed"
  ],
  "author": "Hannes Diercks <node@xiphe.net> (https://xiphe.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xiphe/cachified/issues"
  },
  "homepage": "https://github.com/Xiphe/cachified#readme",
  "devDependencies": {
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "esbuild": "0.14.49",
    "jest": "28.1.2",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  }
}
